<!DOCTYPE html>
<html lang="en">

<head>
  <%- include("../header"); %>
  <style>
    body {
      font-family: "Nanum Gothic", sans-serif;
    }

    #createForm {
      width: 90%;
      height: 100%;
      padding: 5px;
      margin: 0 auto;
      background: linear-gradient(#e66465, #9198e5);
    }

    .form-wrap {
      background-color: white;
      width: 100%;
      height: 100%;
      margin: auto;
      padding: 10px;
    }

    .tips {
      font-size: 9pt;
    }

    .tips-border {
      width: 100%;
      height: 2px;
      background-color: gray;
      margin-bottom: 20px;
    }

    .md-form {
      margin: 30px 0;
    }

    #create_button {
      width: 100px;
      transition: 0.3s;
      background-color: #7D99ED !important;
    }

    #create_button:hover {
      background-color: #7354F4 !important;
      transition: 0.3s;
    }

    
  </style>
</head>


<body>
  <%- include("../menu"); %>
  
  <%result.forEach(music => { %>
  <div id="createForm">

    <div class="form-wrap">
      <p><%= music.where %></p>
    </div>

  </div>
  <% }); %>

  <%- include("../footer"); %>
  <script>
  </script>
</body>

</html>